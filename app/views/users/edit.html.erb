<h1> This is the User Edit.html.erb file </h1>

<%= form_for @user, url: user_path(@user), method: :put do |f| %>

 

	<p>
		<%=  f.label :name %>
		<%= f.text_field :name %>
	</p>

	<p>
		<%= f.label :email %>
		<%= f.text_field :email %>
	</p>

	
	<p>
	<%= f.submit %>
	</p>

<% end %>

<%= link_to 'Back', users_path %>