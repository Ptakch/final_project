<div id='videoplayer'>

<!-- <%# @videos.each do |v| %>
<p><%#= v.id %> : <%#= v.name %> : <%#= v.description.html_safe %></p>
<%# end %> -->

<% @all_projects.each do |project| %>
	<h1><%= project.name %></h1>

	<% project.videos.reverse_each do |video| %>
		<%= link_to "<p>#{ video.name } : #{ video.description.html_safe }</p>".html_safe, video%>
		<%= image_tag video.thumbnail %>
	<% end %>
<% end %>

</div>